{
    "source": ["tokens/tokens.json"],
    "platforms": {
      "scss": {
        "transformGroup": "scss",
        "prefix": "upl",
        "buildPath": "resources/scss/common/",
        "files": [
          {
            "destination": "_variables.scss",
            "format": "scss/variables"
          }
        ]
      },
      "css": {
        "transformGroup": "css",
        "prefix": "upl",
        "buildPath": "resources/css/common/",
        "files": [
          {
            "destination": "_variables.css",
            "format": "css/variables"
          }
        ]
      }
    }
}